//FIND AN ELEMENT IN 2D ARRAY
#include<iostream>
using namespace std;
int main()
{
    int n,m;
    int i,j;
    cin>>n>>m;
    int arr[n][m];
    cout<<"Enter the elements in array"<<endl;
    for(int i=0;i<n;i++)
    {
        for(int j=0;j<m;j++)
        {
            cin>>arr[i][j];
        }
    }
    cout<<"Display the element"<<endl;
    for(int i=0;i<n;i++)
    {
        for(int j=0;j<m;j++)
        {
            cout<<arr[i][j]<<" ";
        }cout<<endl;
    }
    int x;
    cout<<"The search element will be"<<endl;
    cin>>x;
    bool flag=false;
    for(int i=0;i<n;i++)
    {
        for(int j=0;j<m;j++)
        {
            if(arr[i][j]==x)
            {
                cout<<i<<" "<<j<<" "<<endl;
                flag=true;
            }
        }
    }
    if(flag)
    {
        cout<<"Required element will be found"<<endl;
    }
    else
    {
        cout<<"Not found"<<endl;
    }
    return 0;
}
